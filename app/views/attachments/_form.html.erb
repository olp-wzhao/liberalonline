<%= simple_form_for(@attachment) do |f| %>
  <%= f.error_notification %>
    <%= f.input :icon_uri %>
    <%= f.input :language %>
    <%= f.input :upload_date %>
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :place %>
    <%= f.input :database_name %>
    <%= f.input :file_location %>
    <%= f.input :attachment_type %>
    <%= f.input :published %>
    <%= f.input :web_site %>
    <%= f.input :updated_time %>

    <%= f.button :submit %>

<% end %>