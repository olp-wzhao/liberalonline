					<div id="imageholder"><img src="/photos/mpp/mpp_photos/<%= (cabinet_mpp.riding_id + 9000).to_s %>_thumbnail.jpg" class="drop-shadow lifted"></div>
					<div style="height:200px;">
					<div class="listh1"><%= cabinet_mpp.first_name %> <%= cabinet_mpp.last_name %></div>
					<div class="listh2"><%= raw(cabinet_mpp.cabinet_minister_en) %></div>
					</div>
				</div>	
			<% end %>
		<% end %>




